<h1>ALL JOURNAL ENTRIES - This is index.erb. We are viewing the posts</h1>
<h4>Show Name</h4>
<h4>Show Description</h4>
<h4>Rating</h4> 

<% @posts.each do |post| %>
  <%= post.created_at %>
  <h4> <a href="/posts/<%= post.id %>" <%= post.title %> </h4>
  <%= post.content %>
  <%= post.tact_rating %>
<% end %>